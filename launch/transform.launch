<launch>
    <node pkg="tf" type="static_transform_publisher" name="world2map"
          args="0 0 0 0 0 0 world map 100"/>

    <!-- TF for 3D lidar-->
    <!-- 3Dlidar_link是3D激光雷达坐标系 -->
    <!-- 机体系与3Dlidar_link的坐标转换关系见 amov_solo_3Dlidar.sdf -->
    <node pkg="tf" type="static_transform_publisher" name="tf_3Dlidar"
          args="0 0 0.1 0 0 0 base_link rplidar::link 100"/>

    <!-- TF for realsense_camera-->
    <!-- realsense_camera_link是D435i坐标系 -->
    <!-- 机体系与realsense_camera_link的坐标转换关系见 P300_D435i.sdf -->
    <node pkg="tf" type="static_transform_publisher" name="base2camera"
          args="0.1 0 0 0 0 0 base_link stereo_camera::link 100"/>
</launch>
