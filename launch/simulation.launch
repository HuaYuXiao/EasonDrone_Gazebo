<launch>
    <!-- parameter for planner -->
<!--    <arg name="model" value="iris_realsense_livox"/>-->
    <arg name="model" value="p450_3Dlidar_D435i"/>

    <!-- world -->
    <include file="$(find prometheus_gazebo)/launch/GFKD.launch"/>

    <!-- 1号无人机 -->
    <include file="$(find prometheus_gazebo)/launch/sitl_$(arg model).launch"/>

    <include file="$(find uav_control)/launch/uav_control_indoor.launch"/>

    <!-- STATION -->
    <include file="$(find prometheus_station)/launch/prometheus_station.launch"/>

    <!-- 启动octomap建图 -->
    <include file="$(find ego_planner)/launch/mapping.launch"/>

    <!-- 启动rviz -->
    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find ego_planner)/config/EGO-Planner.rviz"/>
</launch>
